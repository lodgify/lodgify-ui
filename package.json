{
  "name": "lodgify-ui",
  "version": "0.1.0",
  "description": "React components for building amazing websites with Lodgify",
  "main": "dist/index.js",
  "config": {
    "styleguide_config_path": "./styleguide/config",
    "styleguide_dist_path": "./styleguide/dist"
  },
  "scripts": {
    "copy:gh-pages": "cp -R $npm_package_config_styleguide_dist_path/* ./gh-pages",
    "semantic:watch": "gulp watch --gulpfile lodgify-ui/src/semantic/gulpfile.js",
    "semantic:build": "gulp build --gulpfile lodgify-ui/src/semantic/gulpfile.js",
    "lint": "eslint . --ext .js",
    "lint:debug": "npm run lint -- --debug",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "patch-package",
    "styleguide:build": "styleguidist build --config $npm_package_config_styleguide_config_path",
    "styleguide:dev-server": "styleguidist server --config $npm_package_config_styleguide_config_path",
    "docs:pr": "npm run lint && npm run test && npm run semantic:build && npm run styleguide:build",
    "docs:build": "npm run docs:pr && npm run copy:gh-pages",
    "test": "jest --coverage --config tools/jest/jest.config.js",
    "test:no-cache": "npm run test -- --no-cache",
    "test:watch": "jest --config tools/jest/jest.config.js --watch",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lodgify/lodgify-ui.git"
  },
  "author": "Lodgify",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lodgify/lodgify-ui/issues"
  },
  "homepage": "https://lodgify.github.io/lodgify-ui/",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.3",
    "babel-plugin-module-resolver": "^3.1.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.18.1",
    "eslint-config-prettier": "^2.6.0",
    "eslint-import-resolver-babel-module": "^4.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "file-loader": "^1.1.9",
    "gulp": "^3.9.1",
    "jest": "^22.4.2",
    "npm": "^5.7.1",
    "patch-package": "^5.1.1",
    "prettier": "^1.10.0",
    "react-docgen": "^2.20.0",
    "react-styleguidist": "^6.2.4",
    "semantic-ui": "^2.3.0",
    "style-loader": "^0.20.2",
    "webpack": "^3.11.0"
  },
  "dependencies": {
    "lodash": "^4.17.5",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "semantic-ui-react": "^0.78.3"
  }
}
